{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "serve:dev": {
      "cache": false
    },
    "prepare:build": {
      "inputs": ["src/svg/**/*.svg", "latte.config.json", "src/**/*.ts", "src/**/*.hbs", "src/**/*.ts"]
    },
    "build:dev": {
      "cache": false,
      "dependsOn": ["prepare:build", "^build:dev"]
    },
    "generate:dist": {
      "cache": false,
      "dependsOn": ["^build:dev", "^generate:dist"]
    }
  }
}
